{
  "name": "type-to-fast-check",
  "version": "0.0.0",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomer@aberba.ch",
    "url": "https://tomeraberba.ch"
  },
  "description": "Automatically convert TypeScript types to fast-check arbitraries!",
  "keywords": [
    "arbitraries",
    "codegen",
    "fast-check",
    "testing",
    "transformer",
    "types",
    "typescript"
  ],
  "homepage": "https://github.com/TomerAberbach/type-to-fast-check",
  "repository": "TomerAberbach/type-to-fast-check",
  "bugs": {
    "url": "https://github.com/TomerAberbach/type-to-fast-check/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/TomerAberbach"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">= 22"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./transformer": {
      "types": "./dist/transformer.d.ts",
      "default": "./dist/transformer.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier --cache --write .",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/ --fix .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "bench": "vitest bench",
    "build": "tsdown",
    "prepublishOnly": "pnpm build"
  },
  "prettier": "@tomer/prettier-config",
  "dependencies": {
    "keyalesce": "^3.0.0",
    "svkc": "^2.0.0"
  },
  "peerDependencies": {
    "fast-check": "^4",
    "typescript": "^5"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.2.2",
    "@tomer/eslint-config": "^4.3.0",
    "@tomer/prettier-config": "^4.0.0",
    "@types/jsesc": "^3.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "fast-check": "^4.3.0",
    "import-from-string": "^0.0.5",
    "jest-extended": "^6.0.0",
    "jsesc": "^3.1.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "tsdown": "^0.15.2",
    "type-to-fast-check": "link:",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
