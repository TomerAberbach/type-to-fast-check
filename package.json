{
  "name": "typescript-fast-check",
  "version": "0.0.0",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomer@aberba.ch",
    "url": "https://tomeraberba.ch"
  },
  "description": "A first-rate module.",
  "keywords": [],
  "homepage": "https://github.com/TomerAberbach/typescript-fast-check",
  "repository": "TomerAberbach/typescript-fast-check",
  "bugs": {
    "url": "https://github.com/TomerAberbach/typescript-fast-check/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/TomerAberbach"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">= 22"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier --cache --write .",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/ --fix .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "bench": "vitest bench",
    "build": "tsdown",
    "prepublishOnly": "pnpm build"
  },
  "prettier": "@tomer/prettier-config",
  "dependencies": {
    "fast-check": "^4.3.0",
    "keyalesce": "^3.0.0",
    "svkc": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.2.2",
    "@tomer/eslint-config": "^4.3.0",
    "@tomer/prettier-config": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "jest-extended": "^6.0.0",
    "jsdom": "^27.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "tsdown": "^0.15.2",
    "vitest": "^3.2.4"
  }
}
